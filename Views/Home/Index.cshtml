                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Email Address" required="required">
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Country" required="required">
                                </div>
                                <div class="form-group text-center">
                                    <button type="submit" class="btn btn-custom btn-sm btn-block">Send</button>
                                </div>
                                <span class="help-block m-b-0 m-t-20 text-muted"><small>By registering you agree to Obrigado <a href="#">Terms of Use</a></small></span>
                            </form>
                        </div><!-- end col -->
